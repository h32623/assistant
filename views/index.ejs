     <tr>
     <td>email 계정</td>
     <td><input name="emailId" placeholder="ratatouille" /></td>
     <td>@</td><td><input name="domain" placeholder="example.com" /></td>
     </tr>
     <tr>
         <td>비밀번호</td>
         <td><input name="password" placeholder="*****" /></td>
     </tr>
     <tr>
         <td>이름</td>
         <td><input name="name" placeholder="라따뚜이" /></td>
     </tr>
    <tr>
      <td>
        <button id="done" value="Done" name="done">DONE </button>
      </td>
    </tr>
  </table>
 </form>
</body>
</html>

